{
  "name": "fairhub.io",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite --port 3000",
    "mock:dev:api": "run-p dev mock:api ",
    "mock:api": "node --watch ./scripts/server.mjs",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --ignore-path .gitignore \"./**/*.+(vue|js|jsx|cjs|mjs|ts|tsx|cts|mts)\" --write",
    "generate:mermaid": "node ./scripts/generate-diagrams.mjs",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "commit": "cz",
    "postinstall": "run-script-os",
    "postinstall:win32": "husky install",
    "postinstall:darwin:linux": "husky install && chmod ug+x .husky/*  && chmod ug+x .git/hooks/*",
    "prepare": "run-script-os",
    "prepare:win32": "husky install",
    "prepare:darwin:linux": "husky install && chmod ug+x .husky/*  && chmod ug+x .git/hooks/*"
  },
  "lint-staged": {
    "./**/*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.15",
    "@vavt/md-editor-extension": "^3.0.0",
    "axios": "^1.1.3",
    "blurhash": "^2.0.5",
    "code-tag": "^1.1.0",
    "dayjs": "^1.11.9",
    "eslint-plugin-prettier": "^4.2.1",
    "filesize": "^10.0.12",
    "jwt-decode": "^3.1.2",
    "marked": "^4.2.12",
    "md-editor-v3": "^4.3.0",
    "mermaid": "^10.4.0",
    "nanoid": "^4.0.2",
    "pinia": "^2.0.21",
    "pinia-plugin-persistedstate": "^3.0.2",
    "sanitize-html": "^2.11.0",
    "storybook-tailwind-foundations": "^1.1.2",
    "validator": "^13.9.0",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5",
    "vue3-lottie": "^3.0.0",
    "vue3-marquee": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@fairdataihub/config": "^2.2.1",
    "@faker-js/faker": "^8.0.2",
    "@hapi/hapi": "^21.3.2",
    "@iconify/vue": "^4.1.0",
    "@mermaid-js/mermaid-cli": "^10.4.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@storybook/addon-a11y": "^6.5.15",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addon-storysource": "^6.5.15",
    "@storybook/builder-vite": "^0.2.6",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.15",
    "@tailwindcss/typography": "^0.5.7",
    "@types/jsdom": "^20.0.0",
    "@types/marked": "^4.0.8",
    "@types/node": "^18.11.5",
    "@types/sanitize-html": "^2.9.0",
    "@types/validator": "^13.7.12",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "commitizen": "^4.2.5",
    "cypress": "^10.7.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sort-keys-custom-order": "^1.0.5",
    "eslint-plugin-storybook": "^0.6.8",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.0",
    "jsdom": "^20.0.0",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^13.0.3",
    "megasanjay-devmoji": "^1.2.1",
    "naive-ui": "^2.33.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "run-script-os": "^1.1.6",
    "start-server-and-test": "^1.14.0",
    "storybook-dark-mode": "^2.0.5",
    "tailwindcss": "^3.1.8",
    "tailwindcss-debug-screens": "^2.2.1",
    "typescript": "~4.7.4",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vfonts": "^0.0.3",
    "vite": "^3.0.9",
    "vitest": "^0.23.0",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.40.7"
  }
}

    "jsx": "preserve",
    "allowJs": true,
    "types": ["naive-ui/volar", "node"],
    "resolveJsonModule": true,
    "esModuleInterop": true,

    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "composite": true,
  "files": [],
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue", "src/**/*.json", "types"],
  "references": [
    {
      "path": "./tsconfig.config.json"
    },
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.vitest.json"
    }
  ]
}
